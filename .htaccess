RewriteEngine on
RewriteRule ^page/([-a-zA-Z0-9_]+)/? index.php?p=$1 [L]
RewriteRule ^cat/([-a-zA-Z0-9_]+)/? index.php?z=$1 [L]
RewriteRule ^404/? error.php
RewriteRule ^search/? search.php
RewriteRule ^registration_bd/? registration_bd.php
RewriteRule ^registration/? registration.php
RewriteRule ^autoriz/? autoriz.php
RewriteRule ^lk/? lk.php